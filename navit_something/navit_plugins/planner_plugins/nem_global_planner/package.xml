    <version>1.17.1</version>
    <description>
        A global_path_plan library and node.
    </description>
    <maintainer email="dingzhiyu@yijiahe.com">dingzhiyu</maintainer>
    <maintainer email="yangchuan@yijiahe.com">yangchuan</maintainer>
