<?xml version="1.0"?>
<package format="2">
  <name>prm_planner</name>
  <version>0.0.0</version>
  <description>The prm planner package</description>

  <!-- One maintainer tag required, multiple allowed, one person per tag -->
  <!-- Example:  -->
  <!-- <maintainer email="jane.doe@example.com">Jane Doe</maintainer> -->
  <maintainer email="max@todo.todo">max</maintainer>


  <!-- One license tag required, multiple allowed, one license per tag -->
  <!-- Commonly used license strings: -->
  <!--   BSD, MIT, Boost Software License, GPLv2, GPLv3, LGPLv2.1, LGPLv3 -->
  <license>TODO</license>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>navit_msgs</build_depend>
  <build_depend>navit_router</build_depend>
  <build_depend>path_smoother</build_depend>
  <build_depend>navit_core</build_depend>

  <build_export_depend>roscpp</build_export_depend>
  <build_export_depend>std_msgs</build_export_depend>
  <build_export_depend>navit_msgs</build_export_depend>
  <build_export_depend>navit_router</build_export_depend>
  <build_export_depend>path_smoother</build_export_depend>
  <build_export_depend>navit_core</build_export_depend>

  <exec_depend>roscpp</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>navit_msgs</exec_depend>
  <exec_depend>navit_router</exec_depend>
  <exec_depend>path_smoother</exec_depend>
  <exec_depend>navit_core</exec_depend>

  <!-- The export tag contains other, unspecified, tags -->
  <export>
    <!-- Other tools can request additional information be placed here -->
    <navit_core plugin="${prefix}/prm_planner.xml" />
  </export>
</package>
