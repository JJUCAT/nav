<launch>
    <arg name="manager_name" default="nodelet_manager" />
    <node pkg="nodelet" type="nodelet" name="$(arg manager_name)" args="manager" output="screen"/>

    <rosparam file="$(find integrated_navigation)/config/sensor/sensor.yaml" command="load" />
    <node pkg="nodelet" type="nodelet" name="MultiSensorDriverNodelet"
          args="load integrated_navigation/MultiSensorDriverNodelet $(arg manager_name)" output="screen">
    </node>
</launch>