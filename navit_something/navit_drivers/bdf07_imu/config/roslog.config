log4j.rootLogger=DEBUG, logFile

log4j.appender.logFile=org.apache.log4j.RollingFileAppender
log4j.appender.logFile.Threshold=DEBUG
log4j.appender.logFile.ImmediateFlush=true
log4j.appender.logFile.Append=true
log4j.appender.logFile.File=${HOME}/resource/log/algorithm/sensor_driver/bdf07_imu.log
log4j.appender.logFile.MaxFileSize=10MB
log4j.appender.logFile.MaxBackupIndex=5
log4j.appender.logFile.layout=org.apache.log4j.PatternLayout
#log4j.appender.logFile.layout.ConversionPattern = [%-5p] [%-23d{yyyy/MM/dd HH:mm:ss:SSS}] [%l]: %m%n
log4j.appender.logFile.layout.ConversionPattern = [%-5p] [%-23d{yyyy/MM/dd HH:mm:ss:SSS}{GMT+8}] [%C:%L]: %m%n
# Set the default ros output to warning and higher
log4j.logger.ros=INFO

# Override my package to output everything
log4j.logger.ros.bdf07_imu=INFO
