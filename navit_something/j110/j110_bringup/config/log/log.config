log4j.logger.ros.navit_localplanner=INFO,navit_localplanner_log
log4j.appender.navit_localplanner_log=org.apache.log4j.RollingFileAppender
log4j.appender.navit_localplanner_log.MaxFileSize=10MB
log4j.appender.navit_localplanner_log.MaxBackupIndex=5
log4j.appender.navit_localplanner_log.Threshold=INFO
log4j.appender.navit_localplanner_log.File=/media/ssd/log/navit_localplanner.log
log4j.appender.navit_localplanner_log.layout=org.apache.log4j.PatternLayout
log4j.appender.navit_localplanner_log.Append=true
log4j.appender.navit_localplanner_log.ImmediateFlush=false 
log4j.appender.navit_localplanner_log.layout.ConversionPattern = [%-5p] [%-23d{yyyy/MM/dd HH:mm:ss:SSS}{GMT+8}] [%C:%L]: %m%n

