<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
    把 costmap_common_params 参数加载到相应的 ns 空间内
    <rosparam file="$(find rrt_ssplanner)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find rrt_ssplanner)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find rrt_ssplanner)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find rrt_ssplanner)/config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find rrt_ssplanner)/config/local_planner_params.yaml" command="load" />
    <rosparam file="$(find rrt_ssplanner)/config/global_planner_params.yaml" command="load" />
  </node>
</launch>